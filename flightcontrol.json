{
  "environments": [
    {
      "id": "production",
      "name": "Production",
      "region": "eu-north-1",
      "source": { "branch": "main" },
      "services": [
        {
          "id": "blitz-app-ybriyO",
          "cpu": 0.25,
          "name": "Blitz App",
          "port": 3000,
          "type": "fargate",
          "memory": 0.5,
          "basePath": ".",
          "buildType": "nixpacks",
          "buildCommand": "blitz build && blitz prisma migrate deploy",
          "envVariables": {},
          "maxInstances": 1,
          "minInstances": 1,
          "startCommand": "blitz start",
          "healthCheckPath": "/",
          "enableCloudfrontSwr": true
        },
        {
          "id": "database-MoNOaf",
          "name": "Database",
          "port": 5432,
          "type": "rds",
          "engine": "postgres",
          "private": false,
          "storage": 20,
          "maxStorage": 100,
          "instanceSize": "db.t4g.micro",
          "engineVersion": "13",
          "deletionProtection": false,
          "applyChangesImmediately": false,
          "autoUpgradeMinorVersions": true,
          "connectionStringEnvVarName": "DATABASE_URL"
        }
      ]
    },
    {
      "id": "main-manual",
